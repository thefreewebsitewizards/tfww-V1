  function goToNextQuestion() {
    if (currentQuestion < totalQuestions) {
      if (currentQuestion == 1) {
        $(".left-nav-btn").removeClass("is-hidden");
      }

      if (currentQuestion == 4) {
        $(".right-nav-btn").addClass("is-hidden");
      }

      //focus on next input field
      $(this).closest(".question-container").next().find(".input-box").focus();

      $("#question-" + currentQuestion)
        .delay(500)
        .slideUp()
        .addClass("question-inactive");
      currentQuestion++;
      setModalImage();
      setMobileBackgroundImage();
      $("#question-" + currentQuestion)
        .delay(500)
        .slideDown()
        .removeClass("question-inactive");
    }
  }

  function goToPreviousQuestion() {
    if (currentQuestion > 1) {
      if (currentQuestion === 2) {
        $(".left-nav-btn").addClass("is-hidden");
      }

      if (currentQuestion === 5) {
        $(".right-nav-btn").removeClass("is-hidden");
      }

      $("#question-" + currentQuestion)
        .delay(500)
        .slideUp()
        .addClass("question-inactive");

      currentQuestion--;
      setModalImage();
      setMobileBackgroundImage();
      $("#question-" + currentQuestion)
        .delay(500)
        .slideDown()
        .removeClass("question-inactive");
    }
  }

